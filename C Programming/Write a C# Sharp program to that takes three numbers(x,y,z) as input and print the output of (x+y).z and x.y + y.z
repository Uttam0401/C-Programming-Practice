//Write a C# Sharp program to that takes three numbers(x,y,z) as input and print the output of (x+y).z and x.y + y.z
  #include<stdio.h>
  int main()
  {
    int x,y,z,result1,result2;
    printf("Enter The Value Of X,Y,Z:-");
    scanf("%d %d %d",&x,&y,&z);
    result1=x+y*z;
    result2=x*y+y*z;
    printf("Enter The Value of(x+y).z=%d",result1);
    printf("\nEnter The Value Of x.y + y.z=%d",result2);
    return 0;
    
  }